<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>Result</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">      
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
  
  </head>
  <body>
    
    <nav class="nav-extended">
      <div class="nav-wrapper blue">
        <a href="#" class="brand-logo center">Transcript Parser</a>
        <ul class="left">
          <li><a href="/">Go Back</a></li>
        </ul>    
      </div>
    </nav>

    <main class="container">
        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="card horizontal">
                      
                        <div class="card-stacked">
                            <div class="card-content">
                                <p>Name: <%= data.fullname %></p>
                                <p>ID: <%= data.id %></p>
                                <p>GPA: <%= data.gpa %></p>
                                <p>COMP2606: <%= data.comp2602 %></p>
                                <p>INF2602: <%= data.info2602 %></p>
                                <p>COMP2611: <%= data.comp2611 %></p>
                                <p>COMP2605: <%= data.comp2605 %></p>
                            </div>
                        </div>
                    </div>
                </div>

              <div class="row">
                
                <div class="input-field col s12">
                  <textarea id="textarea2" style="height: 90vh;" class="materialize-textarea">
                    <%= JSON.stringify(parsedText) %>
                    </textarea>
                <label for="textarea1">Parsed Contents</label>
                </div>
              </div>
            </form>
          </div>
    </main>

    
  </body>
</html>